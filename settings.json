{
  "disableAllHooks": false,
  "permissions": {
    "allow": [
      "mcp__plugin_serena_serena__think_about_whether_you_are_done",
      "mcp__plugin_serena_serena__think_about_collected_information",
      "mcp__plugin_serena_serena__list_memories",
      "mcp__plugin_serena_serena__activate_project",
      "mcp__plugin_serena_serena__write_memory",
      "WebSearch",
      "Bash(cat:*)",
      "Bash(python3:*)",
      "Bash(python:*)",
      "Bash(pip:*)",
      "Bash(pipx:*)",
      "Bash(claude:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(where:*)",
      "Bash(superclaude:*)",
      "mcp__plugin_playwright_playwright__browser_click",
      "mcp__plugin_playwright_playwright__browser_take_screenshot",
      "mcp__plugin_playwright_playwright__browser_wait_for",
      "mcp__godot__run_project",
      "mcp__godot__get_debug_output",
      "mcp__godot__launch_editor",
      "Bash(npm update:*)"
    ],
    "defaultMode": "default"
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "npx --yes tsx C:/Users/User/.claude/hooks/skill-activation-prompt.ts"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "TodoWrite",
        "hooks": [
          {
            "type": "command",
            "command": "python3 C:/Users/User/.claude/hooks/taskmaster-sync.py"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 C:/Users/User/.claude/hooks/stop-build-check-enhanced.py"
          },
          {
            "type": "command",
            "command": "python3 C:/Users/User/.claude/hooks/trigger-build-resolver.py"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 C:/Users/User/.claude/hooks/pre-compact-handoff.py"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "agent-sdk-dev@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "feature-dev@claude-plugins-official": true,
    "security-guidance@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "explanatory-output-style@claude-plugins-official": false,
    "learning-output-style@claude-plugins-official": false,
    "frontend-design@claude-plugins-official": true,
    "ralph-wiggum@claude-plugins-official": false,
    "hookify@claude-plugins-official": false,
    "plugin-dev@claude-plugins-official": true,
    "serena@claude-plugins-official": true,
    "playwright@claude-plugins-official": true,
    "github@claude-plugins-official": false,
    "supabase@claude-plugins-official": false,
    "atlassian@claude-plugins-official": false,
    "laravel-boost@claude-plugins-official": false,
    "asana@claude-plugins-official": false,
    "vercel@claude-plugins-official": false,
    "context7@claude-plugins-official": true,
    "greptile@claude-plugins-official": true,
    "sentry@claude-plugins-official": false,
    "typescript-lsp@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "csharp-lsp@claude-plugins-official": true
  }
}
